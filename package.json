{
  "name": "voyage",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "check:hh": "hardhat check",
    "cov": "NODE_ENV=test COVERAGE=true hardhat coverage",
    "build:docker": "./docker/build.sh",
    "compile": "SKIP_TASKS=true hardhat compile",
    "clean": "SKIP_TASKS=true hardhat clean",
    "hardhat:local": "hardhat --network localhost",
    "hardhat:goerli": "hardhat --network goerli",
    "hardhat:tenderly": "hardhat --network tenderly",
    "deploy:local": "yarn hardhat:local deploy:dev",
    "deploy:goerli": "TENDERLY=true yarn hardhat:goerli deploy:dev",
    "deploy:tenderly": "TENDERLY=true yarn hardhat:tenderly deploy:dev",
    "dev": "hardhat node --hostname 0.0.0.0 --watch --no-deploy",
    "dev:deploy": "hardhat node --hostname 0.0.0.0 --watch",
    "node": "hardhat node --hostname 0.0.0.0 --no-deploy --network hardhat",
    "node:deploy": "hardhat node --hostname 0.0.0.0",
    "node:interval": "MINING_MODE=interval yarn run node",
    "node:deploy:interval": "MINING_MODE=interval yarn node:deploy",
    "scripts": "ts-node scripts/runner.ts",
    "test": "NODE_ENV=test hardhat test",
    "test-scenarios": "NODE_ENV=test npx hardhat test test/__setup.spec.ts test/scenario.spec.ts",
    "test:gas": "NODE_ENV=test REPORT_GAS=true hardhat test",
    "format": "yarn format:sol && yarn format:js",
    "format:js": "prettier --config .prettierrc.js.json --write {helpers,deploy,test,scripts,test/helpers,test/helpers/**}/*.ts",
    "format:sol": "hardhat format",
    "scan": "slither --print human-summary .",
    "postinstall": "husky install"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@ethersproject/abi": "^5.6.3",
    "@looksrare/sdk": "^0.8.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@opensea/seaport-js": "^1.0.5",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.0.2",
    "@types/chai": "^4.2.18",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.2",
    "bignumber.js": "^9.0.2",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "enquirer": "^2.3.6",
    "ethers": "^5.7.0",
    "execa": "5.1.1",
    "hardhat": "^2.9.4",
    "hardhat-deploy": "^0.11.4",
    "hardhat-diamond-abi": "^3.0.0",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-watcher": "^2.3.0",
    "husky": "^7.0.4",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.2",
    "yarn-upgrade-all": "^0.5.4"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.6.2",
    "@opengsn/common": "2.2.6",
    "@opengsn/contracts": "^2.2.6",
    "@openzeppelin/contracts": "^4.7.2",
    "@openzeppelin/contracts-upgradeable": "^4.7.2",
    "@pwnednomore/contracts": "^1.0.12",
    "@rari-capital/solmate": "^6.4.0",
    "@tenderly/hardhat-tenderly": "^1.1.4",
    "hardhat-prettier": "^0.0.1",
    "lodash": "^4.17.21",
    "openzeppelin-solidity": "^4.4.1"
  },
  "resolutions": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@ethersproject/bignumber": "^5.6.0"
  }
}
