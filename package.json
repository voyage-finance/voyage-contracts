{
  "name": "voyage",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "check:hh": "hardhat check",
    "cov": "COVERAGE=true hardhat coverage",
    "build:docker": "./docker/build.sh",
    "compile": "hardhat compile",
    "deploy:local": "hardhat --network localhost deploy",
    "deploy:fuji": "hardhat --network avalancheFuji deploy",
    "deploy:voyage": "hardhat --network voyage deploy",
    "dev": "hardhat node --hostname 0.0.0.0 --watch --no-deploy",
    "dev:deploy": "hardhat node --hostname 0.0.0.0 --watch",
    "node": "hardhat node --hostname 0.0.0.0 --no-deploy",
    "node:deploy": "hardhat node --hostname 0.0.0.0",
    "node:interval": "MINING_MODE=interval yarn run node",
    "node:deploy:interval": "MINING_MODE=interval yarn node:deploy",
    "scripts": "ts-node scripts/runner.ts",
    "test": "hardhat test",
    "format": "yarn format:sol && yarn format:js",
    "format:js": "prettier --config .prettierrc.js.json --write {test,scripts}/*.{js,ts} --write {helpers,deploy}/*.ts",
    "format:sol": "hardhat format",
    "scan": "slither --print human-summary .",
    "postinstall": "husky install"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@ethersproject/abi": "^5.6.3",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.0.2",
    "@types/chai": "^4.2.18",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.2",
    "bignumber.js": "^9.0.2",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "enquirer": "^2.3.6",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.6.2",
    "execa": "5.1.1",
    "hardhat": "^2.9.3",
    "hardhat-deploy": "^0.11.4",
    "hardhat-diamond-abi": "^3.0.0",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-watcher": "^2.3.0",
    "husky": "^7.0.4",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.2",
    "yarn-upgrade-all": "^0.5.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.2",
    "@rari-capital/solmate": "^6.4.0",
    "hardhat-prettier": "^0.0.1",
    "lodash": "^4.17.21",
    "openzeppelin-solidity": "^4.4.1"
  }
}
